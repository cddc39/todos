{
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.3.7",
    "postcss-import": "^14.0.2",
    "postcss-url": "^10.1.3",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/babel-preset-app": "^4.5.14",
    "@vue/test-utils": "^2.0.0-rc.16",
    "babel-core": "^7.0.0-bridge.0",
    "cypress": "^8.6.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.19.1",
    "jest": "^26.6.3",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.4",
    "vite": "^2.6.0",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.3.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "extraFileExtensions": [
        ".vue"
      ],
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "overrides": [
      {
        "files": [
          "*.js",
          "*.ts",
          "*.vue"
        ]
      }
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "name": "todo",
  "prettier": {
    "printWidth": 120,
    "semi": false
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "build-container": "docker build --tag 'registry.digitalocean.com/dmikalova/todo:0.0.1' ./",
    "container": "docker run --rm -it 'registry.digitalocean.com/dmikalova/todo:0.0.1'",
    "cypress": "cypress run",
    "dev": "vite",
    "fmt": "prettier --write .",
    "lint": "eslint .",
    "push-container": "docker push 'registry.digitalocean.com/dmikalova/todo:0.0.1'",
    "serve": "vite preview",
    "test": "npm run test:unit",
    "test:e2e": "start-server-and-test dev http-get://localhost:3000 cypress",
    "test:unit": "jest src"
  },
  "version": "0.0.0"
}
